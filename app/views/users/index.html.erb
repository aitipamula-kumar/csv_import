<p style="color: green"><%= notice %></p>

<%= form_with url: import_users_path, method: :post do |form| %>
  <%= form.file_field :file, accpet:".csv"  %>
  <%= form.button "Import" %>  
<% end %>

<h1>Users:
<%= @users.count %>
</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path %>
